@using MedBook.Models.ViewModels



@model List<IndicatorVM>

<div>
    @if (Model.Count == 0)
    {
        <p>Показатель не найден.</p>
    }
    else
    {
        @foreach (var ind in Model)
        {
            <label><a asp-controller="Indicator" asp-action="Edit" asp-route-id="@ind.Id">@ind.Name</a></label><br />
        }
    }
</div>
